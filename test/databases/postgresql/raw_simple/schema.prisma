datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/vnsoft2014/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "raw"
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  username String
}
