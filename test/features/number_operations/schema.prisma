datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/vnsoft2014/prisma-client-go"
	output            = "."
  disableGoBinaries = true
  package           = "db"
}

model Post {
  id     String @id @default(cuid()) @map("_id")
  int    Int
  float  Float
  int2   Int
  float2 Float
}
