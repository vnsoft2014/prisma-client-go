datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/vnsoft2014/prisma-client-go"
	output            = "."
  disableGoBinaries = true
  package           = "pagination"
}

model Post {
  id      String @id @default(cuid()) @map("_id")
  title   String @unique
  content String
}
