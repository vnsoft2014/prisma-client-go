datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/vnsoft2014/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "errors"
}

model User {
  id       String  @id @default(cuid()) @map("_id")
  email    String  @unique
  username String
  name     String?
  stuff    String?
}
